<Window x:Class="AlmacenListanco.BuscadorClientes"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AlmacenListanco"
        mc:Ignorable="d"
        Title="BuscadorClientes" Height="369.141" Width="423.047">
    <Grid>
        <DataGrid x:Name="Clientes_dataGrid" AutoGenerateColumns="False" HorizontalAlignment="Left" Margin="26,24,0,0" VerticalAlignment="Top" Height="276" Width="356" SelectionChanged="Clientes_dataGrid_SelectionChanged">
            <DataGrid.Columns>
                <DataGridTextColumn Header="DNI" Binding="{Binding Path=Id_propietario}"/>
                <DataGridTextColumn Header="Nombre" Binding="{Binding Path=Nombre}"/>
                <DataGridTextColumn Header="Apellidos" Binding="{Binding Path=Apellidos}"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
